@model List<Venta>;

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">No Venta</th>
            <th scope="col">Cliente</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ventas in Model)
        {
            <tr>
                <td>@ventas.ventaId</td>
                <td>@ventas.noVenta</td>
                <td>@ventas.cliente</td>
            </tr>
        }
    </tbody>
</table>